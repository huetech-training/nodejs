{
    "env": {
      "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "func-names": "off",
      "no-underscore-dangle": "off",
      "consistent-return": "off",
      "linebreak-style":["error","windows"]
    }
  }